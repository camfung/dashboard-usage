{
    "name": "tp-bloomland/user-activity-client",
    "description": "PHP client for the User Activity Summary API and WordPress Dashboard Plugin",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "TpBloomland\\UserActivity\\": ["src/", "lib/UserActivity/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TpBloomland\\UserActivity\\Tests\\": "tests/"
        }
    }
}
